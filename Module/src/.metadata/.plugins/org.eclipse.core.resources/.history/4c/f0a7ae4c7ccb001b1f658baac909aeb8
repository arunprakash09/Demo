import java.util.*;
public class Palindrome {
public static void main(String[] args) {
	Scanner sc=new Scanner(System.in);
	int n=sc.nextInt();
	int rem=0,sum=0;
	int temp=n;
	while(temp!=0)
	{
		rem=temp%10;
		sum=sum*10+rem;
		temp/=10;
		
	}
	if(sum==n)
		System.out.println("Palindrome");
	else
		System.out.println("not palindrome");
}
}
