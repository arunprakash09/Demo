import java.util.*;
public class Palindrome {
public static void main(String[] args) {
	Scanner sc=new Scanner(System.in);
	int n=sc.nextInt();
	for(int i=0;i<n/2;i++)
	{
		if(i%n==0)
		{
			flag=1;
			break;
		}
	}
	if(flag==0)
		System.out.println("Prime");
	else
		System.out.println("Not Prime");
}
}
